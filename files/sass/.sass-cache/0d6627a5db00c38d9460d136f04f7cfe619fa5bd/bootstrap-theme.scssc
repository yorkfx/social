.panel-details{
	ul{
		padding: 0;
		clear: both;
		@include clearfix;
		@include background-image(
			linear-gradient(left top, #ddd, #ddd),
			linear-gradient(left top, #ddd, #ddd),
			linear-gradient(left top, #ddd, #ddd),
			linear-gradient(left top, #ddd, #ddd),
			linear-gradient(left top, #ddd, #ddd));
		@include background-size(1px 100%, 1px 100%, 1px 100%, 1px 100%, 1px 100%);
		background-position: 0%, 25%, 50%, 75%, 100%;
		background-repeat:repeat-y;
		li{
			border-bottom:1px solid #ddd;
			list-style: none;
			width: 25%;
			display: block;
			float: left;
			font-size: .93em;
			line-height: 2.6em;
			text-indent: 10px
		}
	}
}

.panel-body{
	ul{
		padding: 0;
		clear: both;
		@include clearfix;
		position: relative;
		&:after{
			content:" ";
			display: block;
			width: 100%;
			height: 4px;
			background-color: #fff;

			position: absolute;
			left: 0;
			bottom: -2px;
		}
		li{
			&:nth-child(3n+0){
				border-right:0 none;
			}

			border-bottom:1px solid #ddd;
			border-right:1px solid #ddd;
			list-style: none;
			width: 33%;
			display: block;
			float: left;
			font-size: .95em;
			line-height: 3em;
			text-indent: 10px
		}
	}
}


.form-control{
	background-color: #f3f6f9;
	border-radius: 4px;
}
.white{
	padding: 0;
	background-color: #FFF;
	margin-bottom: 30px;
	@include border-radius(4px);
	@include clearfix;
	h3, h4, h5, ul, p, form{
		margin: 15px 20px;
		@include clearfix;
	}
	p{
		line-height: 1.8em;
	}
	h4{
		color: #555;
		font-size: 1.1em;
		border-bottom: 1px solid lighten(#000, 80);
		line-height: 2.3em;
	}
}

table{

	.leidos{
		@include opacity(.5);
		background-color: #ddd;
		&:hover{
			background-color: #FFF;
			@include opacity(.8);
		}
	}
}


.mensaje{
	margin-bottom: 20px;
	@include clearfix;
	.cliente{
		position: relative;
		background-color: lighten(#3396d1, 40);
		&:after{
			content:" ";
			width: 25px;
			height: 25px;
			border:1px solid red;
			position: absolute;
			left: -25px;
			top:8px;
			display:block;
			border-style: solid;
			border-width: 12.5px 16px 12.5px 0;
			border-color: transparent lighten(#3396d1, 40) transparent transparent;
		}
	}
	.socio{
		position: relative;
		background-color:#f3f6f9;
		&:after{
			content:" ";
			width: 25px;
			height: 25px;
			border:1px solid red;
			position: absolute;
			left: -25px;
			top:8px;
			display:block;
			border-style: solid;
			border-width: 12.5px 16px 12.5px 0;
			border-color: transparent #f3f6f9 transparent transparent;
		}
	}
	.left{float: left; width:18%;
		h6{line-height: 1.7em;}
	}
	.body{
		float: left;
		width:77%;
		textarea{margin: 10px; width: 96%; background-color: #FFF}
		button{margin: 0 auto; display: block;}
		p{
			margin: 0 30px 30px 30px;
		}
		small{ margin:0; line-height:3.5em; text-indent: 30px; display: block;}
	}
}



figure{
	border:1px solid #fff;
	padding:2px;
	display: inline-block;
	margin:5px;
	position: relative;
	img{
		width: 180px;
		height: 140px;
	}
	figcaption{
		background-color: rgba(0,0,0,.3);
		position: absolute;
		left: 0;
		top: 0;
		width: 100%;
		font-size: 1.4em;
		text-align: center;
		@include opacity(0);
		a{
			color: #fff;
			float: left;
			width: 50%;
			height: 145px;
			line-height: 145px;
			&:first-child{
				border-right:1px solid white;
			}
			&:hover{
				color: #fff;
				font-size: 1.4em;
			}
		}

	}
	&:hover{
		figcaption{
			color: #000;
			@include opacity(1);
		}
	}
}


#jquery-overlay {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 90;
	width: 100%;
	height: 500px;
}
#jquery-lightbox {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 100;
	text-align: center;
	line-height: 0;
}
#jquery-lightbox a img { border: none; }
#lightbox-container-image-box {
	position: relative;
	background-color: #fff;
	width: 250px;
	height: 250px;
	margin: 0 auto;
}
#lightbox-container-image { padding: 10px; }
#lightbox-loading {
	position: absolute;
	top: 40%;
	left: 0%;
	height: 25%;
	width: 100%;
	text-align: center;
	line-height: 0;
}
#lightbox-nav {
	position: absolute;
	top: 0;
	left: 0;
	height: 100%;
	width: 100%;
	z-index: 10;
}
#lightbox-container-image-box > #lightbox-nav { left: 0; }
#lightbox-nav a { outline: none;}
#lightbox-nav-btnPrev, #lightbox-nav-btnNext {
	width: 49%;
	height: 100%;
	zoom: 1;
	display: block;
}
#lightbox-nav-btnPrev {
	left: 0;
	float: left;
}
#lightbox-nav-btnNext {
	right: 0;
	float: right;
}
#lightbox-container-image-data-box {
	font: 10px Verdana, Helvetica, sans-serif;
	background-color: rgba(255,255,255,1);
	margin: 0 auto;
	line-height: 1.4em;
	overflow: auto;
	width: 100%;
	padding: 0 10px 0;
	position: relative;
	top: -45px;
}
#lightbox-container-image-data {
	padding: 0 10px;
	color: #666;
}
#lightbox-container-image-data #lightbox-image-details {
	width: 70%;
	float: left;
	text-align: left;
	position: relative;
}
#lightbox-image-details-caption { font-weight: bold; }
#lightbox-image-details-currentNumber {
	display: block;
	clear: left;
	padding-bottom: 1.0em;
}
#lightbox-secNav-btnClose {
	width: 66px;
	float: right;
	padding-bottom: 0.7em;
	z-index:1000000;
}



div.tagsinput {
	@include border-radius(4px);
	background-color: #fff;
	border:1px solid #CCC;
	padding:5px;
	min-height:260px;
	span.tag {
		@include border-radius(4px);
		background-color: #f3f6f9;
		display: block;
		width: 48.5%;
		float: left;
		padding: 6px;
		text-decoration:none;
		color: #444;
		margin: 2px;
		font-size:.95em;
		font-weight: 100;

		a{
			font-weight: bold;
			color: #444;
			text-decoration:none;
			font-size: .95em;
			font-weight: bolder;
			position: relative;
			top: -1px;
			float: right;
		}
		input {
			margin:0px;
			border:1px solid red;
			padding:0 5px;
			color: #000;
			background-color: #f3f6f9;
			outline:0px;
			margin-right:5px;
			margin-bottom:0px;

		}
		div {
			display:block; float: left;
		}

		.tags_clear { clear: both; width: 100%; height: 0px; border:1px solid red;}
		.not_valid {background: #FBD8DB !important; color: #90111A !important;}
	}
}
#opciones_paquete_addTag{
	input{
		border:1px solid #ddd;
		padding: 5px 7px;
		width: 52%;
		margin: 3px;
		background-color: #fff;
		font-size: .95em;
	}
}

#table_plans{
	width:100%;
	margin:20px;
	text-align: center;
	th[scope="row"]{
		text-indent: 50px;
		font-size: 1.1em;
	}
	th[scope="col"]{
		background-color: #ddd;
		h5{
			font-size: 1.5em;
			margin: 0;
			font-weight: 100;
			text-align: center;
			line-height: 1em;
			margin-top: 20px;
			color: #888;
		}
		span{
			border:4px solid white;
			background-color: #fff;
			display: block;
			margin: 0 auto;
			width: 100px;
			height: 100px;
			text-align: center;
			font-size: 1.3em;
			color: #888;
			padding-top: 28px;
			@include box-shadow(inset 0 10px 10px rgba(0,0,0,.2));
			@include border-radius(50px);
			position: relative;
			bottom: -20px;
			small {
				font-size: 1.1rem;
				display: block;
			}
		}
	}
	td{
		border-bottom:1px solid #ddd;
		border-top:1px solid #fff;
		padding: 35px;
		font-size: 1.2em;
	}

}

#pago_otros{
	img{
		display: block;
		margin: 1em auto;
	}
	h2{
		font-size: 4em;
		text-align: center;
		line-height: 2em;
		color:lighten(#000, 60);
	}
	small{
		display: block;
		text-align: center;
	}
}

.contuplan{
	dt{
		text-indent: 20px;
		line-height: 2.5em;
	}
	dd{
		font-size: 4em;
		line-height: 1.3em;
		text-align: center;
		border-bottom:1px solid #ddd;
		color:lighten(#000, 80);
		small{
			display: block;
			color:lighten(#000, 60);
			font-size: 1rem;
			line-height: 1.2rem;
			margin: 1em;
		}
	}
}

