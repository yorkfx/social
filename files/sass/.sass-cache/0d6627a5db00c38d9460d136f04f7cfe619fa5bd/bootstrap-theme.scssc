@import "compass";
@import "icons";
@import "tabs";
@import "header";
@import "footer";
@import "salon";
@import "range";
@import "listado";
@import "forms";
@import "datepicker";
@import "mixins";

#gallery{
	margin: 8px;
	a{
		margin: 2px 2px 3px 0;
		padding: 4px;
		border:1px solid silver;
		display: inline-block;
		float: left;
		height: 190px;
		width: 220px;
		margin: 5px;
		overflow: hidden;
		img{
			@include transition (all 0.4s ease-in-out);
			@include grayscale(100%);
			width: 100%;
			height: 100%;
	          &:hover{
				@include grayscale(0%);
			}
          }
	}
}
#detail_msg{
	text-align: center;
	i{
		font-size: 2.6em;
		line-height: 70px;
		color: #888;
	}
	border-bottom:1px solid #ddd;
	margin-bottom: 20px;
	padding-bottom: 20px
}
#loading{
	height: 50px;
	display: inline-block;
	margin: 0 auto;
	position: relative;
	left:48%;
	top:-100px;
	background-color: rgba(255,255,255,.7);
	text-align: center;
	color: #A157E4;
	img{
		display: block;
		margin: 0 auto;
	}
}
#mapa_ubicacion{
	margin:20px;
	height:450px;
	position: relative;
	@include transition (all 0.4s ease-in-out);
	&:hover{
		span{@include opacity(1);}
	}
	span{
		a{
			font-size: 1.05em;
			color: #000;
			display: block;
			background-color: rgba(255,255,255,.7);
			padding: 10px;
		}

		width: 100%;
		text-align: center;
		@include opacity(1);
	}
}
@import url(http://fonts.googleapis.com/css?family=Montserrat:400,700);
body{
	margin: 0 !important;
	padding-top: 65px;
	display: block;
	font-family: Montserrat,sans-serif;
	background-color: #F0F3F6;
	color: #555;
	font-weight: 100;
	overflow-x: hidden;
 }
:focus {
	outline: 0 none !important;
}
.modal{
	margin-top: 60px;
}
textarea{
	resize:none !important;
}

.nums{
	border:1px solid #000;
	padding: 4px;
	width:400px;
	padding: 5px 0;
	&.nums-10{
		text-indent:400px/10;
		letter-spacing: calc(20px);
		background-image:url(../img/stripe_f6abb68334ed5c237ea461b9bd81dbcd.png);
		background-repeat: repeat;
		background-position: -1px 0;
	}
}
#lst_caracteristicas{
	label{
		width: 32.33333%;
		margin: 0 1% 0 0;
		line-height: 2em;
		input[type="checkbox"]{
			margin: 0 10px 0 0;
		}
	}
}
