@import "compass";
@import "icons";
// @import "datepicker";
@import "tabs";
@import "header";
@import "footer";
@import "salon";
@import "range";
@import "listado";
@import "forms";


#gallery{
	div{
		a{
			margin: 5px 0;
			img{
				margin: 0 auto;
				width: 100%;
				max-height:140px;
			}
		}
	}
}
#loading{
	height: 50px;
	display: inline-block;
	margin: 0 auto;
	position: relative;
	left:48%;
	top:-100px;
}
#mapa_ubicacion{
	margin:20px;
	position: relative;
	@include transition (all 0.4s ease-in-out);
	&:hover{
		span{@include opacity(1);}
	}
	span{
		a{
			font-size: 1.05em;
			color: #000;
			display: block;
			background-color: rgba(255,255,255,.7);
			padding: 10px;
		}
		position: absolute;
		left: 0;
		bottom: 0;
		width: 100%;
		text-align: center;
		@include opacity(0);
	}
}
@import url(http://fonts.googleapis.com/css?family=Montserrat:400,700);
body{
	margin: 0 !important;
	padding-top: 65px;
	display: block;
	font-family: Montserrat,sans-serif;
	background-color: #F0F3F6;
	color: #555;
	font-weight: 100;
	overflow-x: hidden;
 }
:focus {
	outline: 0 none !important;
}
.modal{
	margin-top: 60px;
}
textarea{
	resize:none !important;
}
.btn_cotiza{
	border:.5px solid darken(#ddd, 10);
	color: darken(#ddd, 20);
	height:66px;
	float:right;
	text-align: center;
	margin: 3px;
	padding: 8px;
	@include transition (all 0.4s ease-in-out);
	@include border-radius(6px);
	@include opacity(.8);
	i{
		@include transition (all 0.4s ease-in-out);
		font-size: 2em;
		color: rgba(0,0,0,.2);
	}
	&:hover{
		background-color: #FFF;
		color: #444;
		@include opacity(1);
		text-decoration: none;
		i{
		color: rgba(255,0,0,1);
		}
	}
}
#showmap{
	border:1px solid white;
	position: relative;
	@include clearfix;
	@include border-radius(8px);
	button{
		position: absolute;
		left: 42%;
		bottom: 20px;
		z-index: 1000;
	}
}
.nums{
	border:1px solid #000;
	padding: 4px;
	width:400px;
	padding: 5px 0;
	&.nums-10{
		text-indent:400px/10;
		letter-spacing: calc(20px);
		background-image:url(../img/stripe_f6abb68334ed5c237ea461b9bd81dbcd.png);
		background-repeat: repeat;
		background-position: -1px 0;
	}
}
.lst_servicios{
	margin: 0;
	padding: 0;
	display:block;
	height:auto;
	li{
		font-size: .95em;
		font-weight: 100;
		list-style: none;
		border-bottom:1px solid #ddd;
		width: 33.33333%;
		float: left;
		line-height: 2.8em;
		background-color: #fff;
		i{
			font-size: 1.4em;
			line-height: 1.8em;
			margin: 0 6px 0 0;
		}
	}

}
#lst_caracteristicas{
	label{
		width: 32.33333%;
		margin: 0 1% 0 0;
		line-height: 2em;
		input[type="checkbox"]{
			margin: 0 10px 0 0;
		}
	}
}
